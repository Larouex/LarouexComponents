{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"build",
				"Packages.sln"
			],
			"group": "build",
			"problemMatcher": [
				"$msCompile"
			]
		},
		{
			"label": "pack",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"pack",
				"Packages.sln",
				"--configuration",
				"Release"
			],
			"group": "build",
			"problemMatcher": [
				"$msCompile"
			],
			"dependsOn": "build"
		},
		{
			"label": "clean",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"clean",
				"Packages.sln"
			],
			"group": "build",
			"problemMatcher": [
				"$msCompile"
			]
		}
	]
}